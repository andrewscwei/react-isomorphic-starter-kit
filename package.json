{
  "name": "react-isomorphic-starter-kit",
  "version": "6.4.0",
  "description": "Experimental React universal/isomorphic app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node build/server",
    "clean": "rimraf build",
    "dev": "npm run clean && NODE_ENV=development npm run build:server && NODE_ENV=development DEBUG=app* nodemon --watch ./build --inspect=5858 build/server & NODE_ENV=development npm run build:server -- --watch",
    "build": "npm run clean && NODE_ENV=production npm run build:client && NODE_ENV=production npm run build:server",
    "build:server": "webpack --colors --profile --config config/build.server.conf.ts --display-error-details",
    "build:client": "webpack --colors --profile --config config/build.client.conf.ts --display-error-details",
    "ship:clean": "./scripts/clean.sh",
    "ship:build": "./scripts/build.sh",
    "ship:run": "./scripts/run.sh",
    "test": "NODE_ENV=test DEBUG=test* mocha --require ts-node/register --timeout 10000 --reporter spec --full-trace --colors --recursive --bail --exit \"tests/**/*.ts\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "helmet": "^3.23.3",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.20",
    "morgan": "~1.10.0",
    "node-polyglot": "^2.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^4.0.0",
    "sitemap": "^6.2.0",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/compression": "1.7.0",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/helmet": "0.0.48",
    "@types/ip": "^1.1.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/node-polyglot": "^2.4.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/require-dir": "^1.0.1",
    "@types/serialize-javascript": "^4.0.0",
    "@types/styled-components": "^5.1.2",
    "@types/supertest": "^2.0.10",
    "@types/webpack": "^4.41.21",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-env": "^1.15.2",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "raw-loader": "^4.0.1",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.1",
    "worker-loader": "^3.0.1"
  }
}
