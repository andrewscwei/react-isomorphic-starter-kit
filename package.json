{
  "name": "react-isomorphic-starter-kit",
  "version": "3.9.0",
  "description": "Experimental React universal/isomorphic app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node build/server",
    "clean": "rimraf build",
    "dev": "npm run clean && NODE_ENV=development npm run build:server && NODE_ENV=development DEBUG=app* nodemon --watch ./build --inspect=5858 build/server & NODE_ENV=development npm run build:server -- --watch",
    "build": "npm run clean && NODE_ENV=production npm run build:client && NODE_ENV=production npm run build:server",
    "build:server": "webpack --colors --profile --config config/build.server.conf.ts --display-error-details",
    "build:client": "webpack --colors --profile --config config/build.client.conf.ts --display-error-details",
    "ship:clean": "./scripts/clean.sh",
    "ship:build": "./scripts/build.sh",
    "ship:run": "./scripts/run.sh",
    "test": "NODE_ENV=test echo \"No tests yet\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "debug": "^4.0.1",
    "dotenv": "^6.0.0",
    "express": "~4.16.3",
    "helmet": "^3.13.0",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "source-map-support": "^0.5.9",
    "styled-components": "^3.4.9",
    "styled-normalize": "^8.0.1"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/debug": "^0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/helmet": "^0.0.42",
    "@types/ip": "^0.0.31",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.11.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-helmet": "^5.0.7",
    "@types/react-intl": "^2.3.11",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-config": "^1.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-transition-group": "^2.0.14",
    "@types/require-dir": "^1.0.0",
    "@types/serialize-javascript": "^1.3.2",
    "@types/webpack": "^4.4.13",
    "@types/webpack-bundle-analyzer": "^2.13.0",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-hot-middleware": "^2.16.4",
    "@types/webpack-manifest-plugin": "^1.3.2",
    "@types/webpack-node-externals": "^1.6.3",
    "copy-webpack-plugin": "^4.5.2",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "image-webpack-loader": "^4.3.1",
    "nodemon": "^1.18.4",
    "raw-loader": "^0.5.1",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.4.0",
    "ts-loader": "^5.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "typescript-styled-plugin": "^0.11.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2"
  }
}
