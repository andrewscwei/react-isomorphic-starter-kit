{
  "name": "react-express-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "start": "node build/server",
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production babel src --minified --source-maps --out-dir=build --copy-files && NODE_ENV=production webpack --colors --profile --config config/build.client.conf.js --display-error-details",
    "dev": "NODE_ENV=development DEBUG=app* nodemon --exec babel-node --verbose --watch src --ignore src/containers --ext js,jsx,json --inspect=5858 src/server",
    "lint": "eslint . -f node_modules/eslint-friendly-formatter --ignore-path .gitignore --ext .jsx --ext .js",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "~4.16.2",
    "helmet": "^3.11.0",
    "i18next": "^10.4.1",
    "i18next-express-middleware": "^1.0.10",
    "i18next-node-fs-backend": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.9.0",
    "prop-types": "^15.6.0",
    "pug": "2.0.0-rc.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-rc.0",
    "react-i18next": "^7.4.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.4.1",
    "eslint": "^4.18.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "nodemon": "^1.15.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0"
  }
}
