{
  "name": "react-isomorphic-starter-kit",
  "version": "8.1.1",
  "description": "Experimental React universal/isomorphic app starter kit",
  "homepage": "https://react-isomorphic-starter-kit.herokuapp.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node build",
    "clean": "rimraf build",
    "dev": "npm run clean && cross-env NODE_ENV=development DEBUG=app* concurrently \"npm run build:server -- --watch\" \"wait-on build/index.js && nodemon --quiet --exec 'node -r dotenv/config' --watch build --inspect=5858 build\"",
    "build": "cross-env-shell NODE_ENV=production \"npm run clean && npm run build:client && npm run build:server\"",
    "build:server": "webpack --config config/build.server.conf.ts",
    "build:client": "webpack --config config/build.client.conf.ts",
    "ship:clean": "./scripts/clean.sh",
    "ship:build": "./scripts/build.sh",
    "ship:run": "./scripts/run.sh",
    "test": "cross-env NODE_ENV=test DEBUG=test* mocha --require ts-node/register --timeout 10000 --reporter spec --full-trace --colors --recursive --bail --exit tests/**/*.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src config tests",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/compression": "1.7.2",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/core-js": "^2.5.5",
    "@types/debug": "^4.1.7",
    "@types/ip": "^1.1.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/require-dir": "^1.0.2",
    "@types/serialize-javascript": "^5.0.2",
    "@types/styled-components": "^5.1.20",
    "@types/supertest": "^2.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-middleware": "^5.0.2",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-hot-middleware": "^2.25.5",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "raw-loader": "^4.0.2",
    "react-is": "^17.0.2",
    "react-refresh": "^0.11.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.2.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "wait-on": "^6.0.0",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.3.0",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-manifest-plugin": "^4.1.1",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "core-js": "^3.20.2",
    "debug": "^4.3.3",
    "dotenv": "^11.0.0",
    "express": "~4.17.2",
    "helmet": "^5.0.1",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "morgan": "~1.10.0",
    "node-polyglot": "^2.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "serialize-javascript": "^6.0.0",
    "sitemap": "^7.1.0",
    "source-map-support": "^0.5.21",
    "styled-components": "^5.3.3",
    "styled-normalize": "^8.0.7"
  }
}
