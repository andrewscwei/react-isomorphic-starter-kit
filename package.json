{
  "name": "react-isomorphic-starter-kit",
  "version": "5.5.0",
  "description": "Experimental React universal/isomorphic app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node build/server",
    "clean": "rimraf build",
    "dev": "npm run clean && NODE_ENV=development npm run build:server && NODE_ENV=development DEBUG=app* nodemon --watch ./build --inspect=5858 build/server & NODE_ENV=development npm run build:server -- --watch",
    "build": "npm run clean && NODE_ENV=production npm run build:client && NODE_ENV=production npm run build:server",
    "build:server": "webpack --colors --profile --config config/build.server.conf.ts --display-error-details",
    "build:client": "webpack --colors --profile --config config/build.client.conf.ts --display-error-details",
    "ship:clean": "./scripts/clean.sh",
    "ship:build": "./scripts/build.sh",
    "ship:run": "./scripts/run.sh",
    "test": "NODE_ENV=test DEBUG=test* mocha --require ts-node/register --timeout 10000 --reporter spec --full-trace --colors --recursive --bail --exit \"tests/**/*.ts\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "~4.17.1",
    "helmet": "^3.20.0",
    "hoist-non-react-statics": "^3.3.0",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "morgan": "~1.9.1",
    "node-polyglot": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.11",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.2.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.7.0",
    "sitemap": "^4.0.2",
    "source-map-support": "^0.5.13",
    "styled-components": "^4.3.2",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "^16.9.0",
    "@types/compression": "1.0.0",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/helmet": "0.0.43",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/ip": "^1.1.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.7.2",
    "@types/node-polyglot": "^0.4.33",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-transition-group": "^4.2.2",
    "@types/require-dir": "^1.0.0",
    "@types/serialize-javascript": "^1.5.0",
    "@types/styled-components": "4.1.8",
    "@types/supertest": "^2.0.8",
    "@types/webpack": "^4.32.1",
    "@types/webpack-bundle-analyzer": "^2.13.2",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-env": "^1.14.0",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "@types/webpack-node-externals": "^1.6.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "copy-webpack-plugin": "^5.0.4",
    "file-loader": "^4.2.0",
    "image-webpack-loader": "^5.0.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "raw-loader": "^3.1.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2",
    "worker-loader": "^2.0.0"
  }
}
