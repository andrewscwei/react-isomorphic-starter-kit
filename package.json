{
  "name": "react-isomorphic-starter-kit",
  "version": "11.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node build",
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "dev": "npm run clean && cross-env NODE_ENV=development concurrently \"npm run build:server -- --watch\" \"wait-on build/index.js && DEBUG=server* nodemon --quiet --exec 'node -r dotenv/config' --watch build build\"",
    "build": "cross-env-shell BUILD_NUMBER=${BUILD_NUMBER:-$(git rev-parse --short HEAD)} \"npm run clean && npm run build:client && npm run build:server\"",
    "build:server": "webpack --config config/build.server.conf.ts",
    "build:client": "webpack --config config/build.client.conf.ts",
    "ship:build": "docker build --build-arg BUILD_NUMBER=${BUILD_NUMBER} --build-arg PUBLIC_PATH=${PUBLIC_PATH} -t ${IMAGE_NAME:-app}:${IMAGE_TAG:-latest} .",
    "ship:run": "docker run -it --rm -p ${PORT:-8080}:8080 --name app ${IMAGE_NAME:-app}:${IMAGE_TAG:-latest}",
    "test": "cross-env SKIP_HTTP=true jest --config=config/test.conf.ts tests/$npm_config_files",
    "test:ts": "jest --silent --testEnvironment=jsdom --config=config/test.conf.ts src/$npm_config_files",
    "export": "ts-node ./scripts/export",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src config tests scripts",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/plugin-transform-runtime": "^7.22.7",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/react": "^14.0.0",
    "@types/compression": "1.7.2",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/debug": "^4.1.8",
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.1",
    "@types/node-polyglot": "^2.4.2",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/supertest": "^2.0.12",
    "@types/webpack": "^5.28.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-env": "^1.18.1",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@types/webpack-node-externals": "^3.0.0",
    "@types/xml": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.44.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "htmlparser2": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.25",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.0.0",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.1",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "core-js": "^3.31.1",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "fast-xml-parser": "^4.2.5",
    "helmet": "^7.0.0",
    "ip": "^1.1.8",
    "morgan": "~1.10.0",
    "node-polyglot": "^2.5.0",
    "normalize.css": "^8.0.1",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1"
  }
}
