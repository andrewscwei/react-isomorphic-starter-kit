{
  "name": "react-isomorphic-starter-kit",
  "version": "7.2.0",
  "description": "Experimental React universal/isomorphic app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node build",
    "clean": "rimraf build",
    "dev": "export NODE_ENV=development DEBUG=app* npm run clean && npm run build:server && nodemon --exec \"node -r dotenv/config\" --watch ./build --inspect=5858 build & npm run build:server -- --watch",
    "build": "export NODE_ENV=production && npm run clean && npm run build:client && npm run build:server",
    "build:server": "webpack --config config/build.server.conf.ts",
    "build:client": "webpack --config config/build.client.conf.ts",
    "ship:clean": "./scripts/clean.sh",
    "ship:build": "./scripts/build.sh",
    "ship:run": "./scripts/run.sh",
    "test": "export NODE_ENV=test DEBUG=test* && mocha --require ts-node/register --timeout 10000 --reporter spec --full-trace --colors --recursive --bail --exit \"tests/**/*.ts\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src config tests",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "core-js": "^3.8.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "helmet": "^4.2.0",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "morgan": "~1.10.0",
    "node-polyglot": "^2.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^5.0.1",
    "sitemap": "^6.3.3",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/compression": "1.7.0",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/core-js": "^2.5.4",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/ip": "^1.1.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.12",
    "@types/node-polyglot": "^2.4.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/require-dir": "^1.0.1",
    "@types/serialize-javascript": "^5.0.0",
    "@types/styled-components": "^5.1.5",
    "@types/supertest": "^2.0.10",
    "@types/webpack": "^4.41.25",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-env": "^1.16.0",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "copy-webpack-plugin": "^7.0.0",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^7.0.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.9.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.1",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-node-externals": "^2.5.2",
    "worker-loader": "^3.0.6"
  }
}
