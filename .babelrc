{
  "sourceMaps": true,
  "retainLines": true,
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel"
  ],
  "env": {
    "server": {
      "plugins": [[
        "css-modules-transform", {
          "generateScopedName": "[hash:6]"
        }
      ], [
        "module-resolver", {
          "root": "./src",
          "alias": {
            "@": "./src"
          }
        }
      ],
      "transform-runtime"
    ]},
    "client": {
      "plugins": [

      ]
    }
  }
}