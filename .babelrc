{
  "sourceMaps": true,
  "retainLines": true,
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel"
  ],
  "env": {
    "server": {
      "plugins": [[
        "css-modules-transform", {
          "generateScopedName": "[name]__[local]--[hash:base64:5]"
        }],
        "transform-runtime"
      ]
    },
    "client": {
      "plugins": [

      ]
    }
  }
}