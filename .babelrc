{
  "sourceMaps": true,
  "retainLines": true,
  "presets": [
    ["env"],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel"
  ],
  "env": {
    "server": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current"
          }
        }]
      ],
      "plugins": [[
        "css-modules-transform", {
          "generateScopedName": "[hash:6]"
        }
      ], [
        "module-resolver", {
          "alias": {
            "@": "./src"
          }
        }
      ],
      "transform-runtime"
    ]},
    "client": {
    }
  }
}