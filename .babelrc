{
  "sourceMaps": true,
  "retainLines": true,
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    ["transform-define", "config/define.conf.js"],
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["module-resolver", {
      "alias": {
        "@": "./src"
      }
    }]
  ],
  "env": {
    "dev": {
      "plugins": [[
        "css-modules-transform", {
          "generateScopedName": "[hash:6]",
          "extensions": [".css", ".pcss"]
        }
      ]]
    }
  }
}